{
  "name": "with-app",
  "version": "1.101.0",
  "private": true,
  "license": "Apache-2.0",
  "dependencies": {
    "@material-ui/core": "^4.9.1",
    "@material-ui/icons": "^4.9.1",
    "@primer/octicons-react": "^9.4.0",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "emoji-mart": "^3.0.0",
    "express": "^4.17.1",
    "firebase": "^7.8.0",
    "framer-motion": "2.0.0-beta.75",
    "fscreen": "^1.0.2",
    "js-cookie": "^2.2.1",
    "lodash.throttle": "^4.1.1",
    "react": "^16.12.0",
    "react-dnd": "^10.0.2",
    "react-dnd-html5-backend": "^10.0.2",
    "react-dom": "^16.12.0",
    "react-modal": "^3.11.2",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.1",
    "react-youtube": "^7.11.3",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "twilio": "^3.48.1",
    "twilio-video": "^2.7.2",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/emoji-mart": "^3.0.2",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/fscreen": "^1.0.1",
    "@types/jest": "^26.0.9",
    "@types/js-cookie": "^2.2.6",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^12.12.54",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-modal": "^3.10.6",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.5",
    "@types/twilio-video": "^2.7.0",
    "@types/uuid": "^8.3.0",
    "@types/youtube": "0.0.39",
    "cypress": "^3.8.3",
    "cypress-multi-reporters": "^1.2.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "husky": "^4.2.5",
    "isomorphic-fetch": "^2.2.1",
    "jest-junit": "^9.0.0",
    "lint-staged": "^10.2.11",
    "mocha": "^6.2.2",
    "mocha-junit-reporter": "^1.23.3",
    "pixelmatch": "^5.1.0",
    "prettier": "^2.0.5",
    "puppeteer": "^2.1.1",
    "react-test-renderer": "^16.12.0",
    "start-server-and-test": "^1.10.8",
    "ts-jest": "^26.2.0",
    "typescript": "^3.9.7"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "start": "cross-env REACT_APP_ANALYTICS_ENV=development REACT_APP_SET_AUTH=passcode react-scripts start",
    "build": "cross-env CI=false REACT_APP_SET_AUTH=passcode react-scripts build",
    "dev": "DISABLE_LOCAL_DEV_AUTH=true netlify dev",
    "test": "jest",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{ts,tsx}",
    "test:ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --browser chrome",
    "cypress:ci": "cross-env CYPRESS_baseUrl=http://localhost:8081 start-server-and-test server http://localhost:8081 cypress:run",
    "deploy:dev": "cross-env REACT_APP_SET_AUTH=passcode npm run build && twilio rtc:apps:video:deploy --authentication=passcode --app-directory ./build --environment=dev",
    "deploy:prod": "cross-env REACT_APP_SET_AUTH=passcode npm run build && twilio rtc:apps:video:deploy --authentication=passcode --app-directory ./build --environment=prod"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-shadow": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "proxy": "http://localhost:8081/",
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
