  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "start": "cross-env REACT_APP_ANALYTICS_ENV=development PORT=3002 craco start",
    "prebuild": "npm run licenses && node ./scripts/write_env.js && cd functions && npm run build:setup",
    "build": "cross-env CI=false REACT_APP_SET_AUTH=passcode craco build",
    "build:client": "cross-env CI=false craco build",
    "dev": "DISABLE_LOCAL_DEV_AUTH=true netlify dev",
    "local:dev": "NODE_ENV=development REACT_APP_SIM_PASS=true REACT_APP_MERCURY_API_HOST='https://localhost:3000' REACT_APP_WITH_API_HOST='https://localhost:3000'  REACT_APP_SOCKET_HOST='wss://localhost:3000' npm run dev",
    "local:staging": "NODE_ENV=staging REACT_APP_SIM_PASS=true REACT_APP_MERCURY_API_HOST='https://test.with.so:8443' REACT_APP_WITH_API_HOST='https://test.with.so:8443' REACT_APP_SOCKET_HOST='wss://test.with.so:8443' npm run dev",
    "test": "jest",
    "test:debug": "node node_modules/.bin/jest --runInBand --watch --bail",
    "test:watch": "jest --watch --bail",
    "test:coverage": "jest --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{ts,tsx}",
    "test:ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --browser chrome",
    "cypress:ci": "cross-env CYPRESS_baseUrl=http://localhost:8081 start-server-and-test server http://localhost:8081 cypress:run",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "console": "node --experimental-repl-await ./scripts/console",
    "licenses": "license-checker --production --direct --json --relativeLicensePath --customPath ./licenseFormat.json --out ./src/pages/licenses/data/licenses.json",
    "clearport": "npx kill-port 3002"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-shadow": "off",
      "no-console": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "proxy": "http://localhost:8888/",
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
