{
  "dependencies": {
    "node-pg-migrate": "^5.3.0",
    "pg": "^8.3.0",
    "redis": "^3.0.2"
  },
  "main": "index.js",
  "scripts": {
    "db:migrate": "./node_modules/.bin/node-pg-migrate --config-file pg/config.js up -m pg/migrations",
    "db:unmigrate": "./node_modules/.bin/node-pg-migrate --config-file pg/config.js down -m pg/migrations",
    "db:new_migration": "./node_modules/.bin/node-pg-migrate --config-file pg/config.js create -m pg/migrations"
  }
}
